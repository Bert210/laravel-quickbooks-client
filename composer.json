{
    "name": "spinen/laravel-quickbooks-client",
    "description": "SPINEN's PHP Client for QuickBooks.",
    "keywords": [
        "quickbooks",
        "client",
        "laravel",
        "spinen"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/database": "^5.5",
        "illuminate/http": "^5.5",
        "illuminate/routing": "^5.5",
        "nesbot/carbon": "~1.20",
        "quickbooks/v3-php-sdk": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1",
        "phpunit/phpunit": "^7.0",
        "psy/psysh": "^0.5.1",
        "satooshi/php-coveralls": "^2.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\QuickBooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\QuickBooks\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\QuickBooks\\Providers\\ServiceProvider",
                "Spinen\\QuickBooks\\Providers\\ClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
